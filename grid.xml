<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Learning TestNG_ Parameters" parallel="tests">

	<listeners>
		<listener class-name="testngTestcases.CustomListener"></listener>
		<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
	</listeners>


	<test name="Chrome Test">
	<parameter name="browser" value="chrome"></parameter>
	<parameter name="url" value="http://google.com"></parameter>	
		
		<classes>

			<class name="seleniumgrid.SeleniumGridDemo" />

		</classes>
	</test> Test
	
	<test name="Firefox Test">
	<parameter name="browser" value="firefox"></parameter>
	<parameter name="url" value="http://google.com"></parameter>	
		<classes>

			<class name="seleniumgrid.SeleniumGridDemo" />

		</classes>
	</test>
	
	<test name="Edge Test">
	<parameter name="browser" value="Edge"></parameter>
	<parameter name="url" value="http://google.com"></parameter>	
		<classes>

			<class name="seleniumgrid.SeleniumGridDemo" />

		</classes>
	</test>
</suite> <!-- Suite -->
